<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>InfinniPlatform</id>
        <version>{INFINNI_PLATFORM_VERSION}</version>
        <authors>Infinnity Solutions</authors>
        <owners>Infinnity Solutions</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>InfinniPlatform</description>
        <copyright>Copyright 2015</copyright>        
    </metadata>
    <files>
        <file src="..\Assemblies\*.dll" target="lib\net45"/>
        <file src="..\Assemblies\*.pdb" target="lib\net45"/>
        <file src="..\Assemblies\*.config" target="lib\net45"/>
        <file src="..\Assemblies\*.exe" target="lib\net45"/>
        <file src="..\InfinniPlatform.SystemConfig\.metadata\**\*.*" target="content\metadata" exclude=""/>
        <file src="PackageContent\Install.bat" target="Install.bat"/>
    </files>
</package>
