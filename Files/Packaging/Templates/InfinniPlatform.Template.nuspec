<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>InfinniPlatform</id>
        <version>{VERSION}</version>
        <authors>Infinnity Solutions</authors>
        <owners>Infinnity Solutions</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>InfinniPlatform. Commit hash: {COMMIT}.</description>
        <copyright>Copyright 2015</copyright>
    </metadata>
    <files>
        <file target="lib\net45" src="*.dll" exclude="*.Tests.dll;InfinniPlatform.SystemConfig.AdministrationCustomization.dll"/>
        <file target="lib\net45" src="*.pdb" exclude="*.Tests.pdb;InfinniPlatform.SystemConfig.AdministrationCustomization.pdb"/>
        <file target="lib\net45\App.config" src="InfinniPlatform.NodeServiceHost.dll.config"/>
        <file target="lib\net45\AppLog.config" src="AppLog.config"/>
        <file target="lib\net45\AppCommon.json" src="AppCommon.json"/>
        <file target="lib\net45\AppExtension.json" src="AppExtension.json"/>
        <file target="content\metadata\Administration" src="content\InfinniPlatform\metadata\Administration\**\*.*"/>
        <file target="content\metadata\Authorization" src="content\InfinniPlatform\metadata\Authorization\**\*.*"/>
    </files>
</package>