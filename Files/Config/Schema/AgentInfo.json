{
    "id":"https://raw.githubusercontent.com/InfinniPlatform/InfinniPlatform/f-infinniClusterTools/Files/Config/Schema/AgentInfo.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "AgentInfo",
    "description": "Информация об агенте (InfinniPlatform.Agent).",
    "type": "object",
    "properties": {
        "Address": {
            "description": "Адрес сервера агента.",
            "type": "string",
            "default": "localhost"
        },
        "Port": {
            "description": "Порт сервера агента.",
            "type": "integer",
            "default": 9900
        },
        "Token": {
            "description": "Токен аутентификации.",
            "type": "string"
        }
    },
    "required": [
        "Address",
        "Port",
        "Token"
    ]
}