{
    "id":"https://raw.githubusercontent.com/InfinniPlatform/InfinniPlatform/f-infinniClusterTools/Files/Config/Schema/Agent.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Agent",
    "description": "Настройки приложения Infinni.Agent.",
    "type": "object",
    "properties": {
        "NodeDirectory": {
            "description": "Путь к приложению Infinni.Node.",
            "type": "string",
            "default": null
        },
        "Token": {
            "description": "Уникальный token для аутентификации при обмене данными между агентом и сервером.",
            "type": "string",
            "default": null
        }
    },
    "required": [
        "NodeDirectory",
        "Token"
    ]
}