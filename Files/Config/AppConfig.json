{
  "$schema": "https://raw.githubusercontent.com/InfinniPlatform/InfinniPlatform/master/Files/Config/Schema/Common.json",
  "app": {
    "AppName": "InfinniPlatform",
    "AppInstance": "",
    "ServerName": "localhost",
    "ServerPort": 9900,
    "StaticFilesMapping": {
      "/metadata": "content/metadata",
      "/": "content/www"
    },
    "EmbeddedResourceMapping": {
    }
  },
  "redisSharedCache": {
    "Host": "localhost",
    "Port": 6379,
    "Database": 0,
    "Password": "TeamCity",
    "ReadBufferSize": 512,
    "WriteBufferSize": 512,
    "ConnectionTimeout": 1000,
    "MaxReconnectRetries": 10,
    "PoolSize": 0
  },
  "mongodbDocumentStorage": {
    "Nodes": [
      "localhost:27017"
    ],
    "UserName": "",
    "Password": ""
  },
  "rabbitmqMessageQueue": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "ManagementApiPort": 15672,
    "PrefetchCount": 1,
    "MaxConcurrentThreads": 200,
    "ReconnectTimeout": 5
  },
  "fileSystemBlobStorage": {
    "BaseDirectory": "%INFINNI_BLOBSTORAGE%"
  },
  "userStorage": {
    "UserCacheTimeout": 30
  },
  "cache": {
    "Type": "Memory"
  },
  "printView": {
    "TempDirectory": "",
    "WkHtmlToPdfPath": "",
    "WkHtmlToPdfArguments": ""
  },
  "quartzScheduler": {
    "ExpireHistoryAfter": null
  },
  "authCookie": {
    "LoginPath": "/Auth/SignInInternal",
    "LogoutPath": "/Auth/SignOut"
  }
}