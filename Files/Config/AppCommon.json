{
  "$schema": "https://raw.githubusercontent.com/InfinniPlatform/InfinniPlatform/master/Files/Config/Schema/Common.json",
  "app": {
    "Name": "InfinniPlatform",
    "InstanceId": "",
    "IsInCluster": false
  },
  "host": {
    "Name": "localhost",
    "Port": 9900
  },
  "redis": {
    "Host": "localhost",
    "Port": 6379,
    "Database": 0,
    "Password": "TeamCity",
    "ReadBufferSize": 512,
    "WriteBufferSize": 512,
    "ConnectionTimeout": 0,
    "MaxReconnectRetries": 10,
    "PoolSize": 0
  },
  "mongodb": {
    "Nodes": [
      "localhost:27017"
    ],
    "UserName": "",
    "Password": ""
  },
  "rabbitmq": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "ManagementApiPort": 15672,
    "PrefetchCount": 1,
    "MaxConcurrentThreads": 200,
    "ReconnectTimeout": 5
  },
  "blobStorage": {
    "BaseDirectory": "${INFINNI_BLOBSTORAGE=BlobStorage}"
  },
  "userStorage": {
    "UserCacheTimeout": 30
  },
  "cache": {
    "Type": "Memory"
  },
  "printView": {
    "HtmlToPdfUtilCommand": "",
    "HtmlToPdfUtilArguments": "",
    "HtmlToPdfTemp": "",
    "TemplatePath": "content\\metadata\\PrintViews"
  },
  "metadata": {
    "DocumentsPath": "content\\metadata\\Documents"
  },
  "scheduler": {
    "ExpireHistoryAfter": null
  },
  "staticContent": {
    "StaticFilesMapping": {
      "/metadata": "content/metadata",
      "/": "content/www"
    },
    "EmbeddedResourceMapping": {},
    "RazorViewsPath": "content/www/RazorViews",
    "EmbeddedRazorViewsAssemblies": []
  },
  "authCookie": {
    "CookieDomain": ""
  },
  "authAdfs": {
    "Enable": false,
    "Server": "Adfs-Server",
    "ResourceUri": ""
  },
  "authGoogle": {
    "Enable": false,
    "ClientId": "Google-Client-Id",
    "ClientSecret": "Google-Client-Secret"
  },
  "authFacebook": {
    "Enable": false,
    "ClientId": "Facebook-Client-Id",
    "ClientSecret": "Facebook-Client-Secret"
  },
  "authVk": {
    "Enable": false,
    "ClientId": "Vk-Client-Id",
    "ClientSecret": "Vk-Client-Secret"
  }
}